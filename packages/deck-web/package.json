{
  "name": "@deck/web",
  "version": "0.1.0",
  "description": "Web bindings for deck of cards animations using react-native-web and Framer Motion",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native-web": "^0.19.10",
    "framer-motion": "^11.0.0"
  },
  "dependencies": {
    "@deck/core": "workspace:*"
  },
  "scripts": {
    "dev": "tsc -b tsconfig.json --watch --preserveWatchOutput",
    "build": "tsc -b tsconfig.json",
    "typecheck": "tsc --noEmit -p tsconfig.json"
  },
  "license": "MIT"
}
